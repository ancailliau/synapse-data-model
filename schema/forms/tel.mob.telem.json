{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "../forms/tel.mob.telem.json",
  "title": "telem",
  "description": "A mobile cell site which a phone may connect to.",
  "properties": {
    "time": {
      "description": null,
      "type": "string"
    },
    "latlong": {
      "description": null,
      "type": "string"
    },
    "http:request": {
      "description": "The HTTP request that the telemetry was extracted from.",
      "$ref": "../forms/inet.http.request.json"
    },
    "host": {
      "description": "The host that generated the mobile telemetry data.",
      "$ref": "../forms/it.host.json"
    },
    "place": {
      "description": "The place representing the location of the mobile telemetry sample.",
      "$ref": "../forms/geo.place.json"
    },
    "loc": {
      "description": "The geo-political location of the mobile telemetry sample.",
      "type": "string"
    },
    "accuracy": {
      "description": "The reported accuracy of the latlong telemetry reading.",
      "type": "number"
    },
    "cell": {
      "description": null,
      "$ref": "../forms/tel.mob.cell.json"
    },
    "cell:carrier": {
      "description": null,
      "$ref": "../forms/tel.mob.carrier.json"
    },
    "imsi": {
      "description": null,
      "type": "string"
    },
    "imei": {
      "description": null,
      "type": "string"
    },
    "phone": {
      "description": null,
      "type": "string"
    },
    "mac": {
      "description": null,
      "$ref": "../forms/inet.mac.json"
    },
    "ipv4": {
      "description": null,
      "type": "string"
    },
    "ipv6": {
      "description": null,
      "type": "string"
    },
    "wifi": {
      "description": null,
      "$ref": "../forms/inet.wifi.ap.json"
    },
    "wifi:ssid": {
      "description": null,
      "$ref": "../forms/inet.wifi.ssid.json"
    },
    "wifi:bssid": {
      "description": null,
      "$ref": "../forms/inet.mac.json"
    },
    "adid": {
      "description": null,
      "$ref": "../forms/it.adid.json"
    },
    "aaid": {
      "description": null,
      "$ref": "../forms/it.os.android.aaid.json"
    },
    "idfa": {
      "description": null,
      "$ref": "../forms/it.os.ios.idfa.json"
    },
    "name": {
      "description": null,
      "$ref": "../forms/ps.name.json"
    },
    "email": {
      "description": null,
      "type": "string"
    },
    "acct": {
      "description": null,
      "$ref": "../forms/inet.web.acct.json"
    },
    "app": {
      "description": null,
      "$ref": "../forms/it.prod.softver.json"
    },
    "data": {
      "description": null,
      "type": "string"
    }
