{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "../forms/inet.servfile.json",
  "title": "servfile",
  "description": "A JARM hash sample taken from a server.",
  "properties": {
    "file": {
      "description": "The file hosted by the server.",
      "type": "string"
    },
    "server": {
      "description": "The inet:addr of the server.",
      "$ref": "../forms/inet.server.json"
    },
    "server:proto": {
      "description": "The network protocol of the server.",
      "type": "string"
    },
    "server:ipv4": {
      "description": "The IPv4 of the server.",
      "type": "string"
    },
    "server:ipv6": {
      "description": "The IPv6 of the server.",
      "type": "string"
    },
    "server:host": {
      "description": "The it:host node for the server.",
      "$ref": "../forms/it.host.json"
    },
    "server:port": {
      "description": "The server tcp/udp port.",
      "$ref": "../forms/inet.port.json"
    }
  },
  "type": "object"
}