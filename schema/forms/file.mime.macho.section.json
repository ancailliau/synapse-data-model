{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "../forms/file.mime.macho.section.json",
  "title": "section",
  "description": "",
  "properties": {
    "segment": {
      "description": "The Mach-O segment that contains this section.",
      "$ref": "../types/file.mime.macho.segment.json"
    },
    "name": {
      "description": "Name of the section.",
      "type": "string"
    },
    "size": {
      "description": "Size of the section in bytes.",
      "type": "integer"
    },
    "type": {
      "description": "The type of the section.",
      "type": "integer"
    },
    "sha256": {
      "description": "The sha256 hash of the bytes of the Mach-O section.",
      "$ref": "../types/hash.sha256.json"
    },
    "offset": {
      "description": "The file offset to the begining of the section",
      "type": "integer"
    }
  },
  "type": "object"
}