{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "../forms/inet.dns.request.json",
  "title": "request",
  "description": "A DNS AAAA wild card record and the IPv6 it resolves to.",
  "properties": {
    "time": {
      "description": null,
      "type": "string"
    },
    "query": {
      "description": null,
      "$ref": "../forms/inet.dns.query.json"
    },
    "query:name": {
      "description": null,
      "type": "string"
    },
    "query:name:ipv4": {
      "description": null,
      "type": "string"
    },
    "query:name:ipv6": {
      "description": null,
      "type": "string"
    },
    "query:name:fqdn": {
      "description": null,
      "type": "string"
    },
    "query:type": {
      "description": null,
      "type": "integer"
    },
    "server": {
      "description": null,
      "$ref": "../forms/inet.server.json"
    },
    "reply:code": {
      "description": "The DNS server response code.",
      "type": "integer"
    },
    "exe": {
      "description": "The file containing the code that attempted the DNS lookup.",
      "type": "string"
    },
    "proc": {
      "description": "The process that attempted the DNS lookup.",
      "$ref": "../forms/it.exec.proc.json"
    },
    "host": {
      "description": "The host that attempted the DNS lookup.",
      "$ref": "../forms/it.host.json"
    }
