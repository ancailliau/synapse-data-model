{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "../forms/proj.sprint.json",
  "title": "sprint",
  "description": "",
  "properties": {
    "name": {
      "description": "The name of the sprint.",
      "type": "string"
    },
    "status": {
      "description": "The sprint status.",
      "type": "string"
    },
    "project": {
      "description": "The project containing the sprint.",
      "$ref": "../types/proj.project.json"
    },
    "creator": {
      "description": "The synapse user who created the sprint.",
      "$ref": "../types/syn.user.json"
    },
    "created": {
      "description": "The date the sprint was created.",
      "type": "string"
    },
    "period": {
      "description": "The interval for the sprint.",
      "type": "string"
    },
    "desc": {
      "description": "A description of the sprint.",
      "type": "string"
    }
  },
  "type": "object"
}